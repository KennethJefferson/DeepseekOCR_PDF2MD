# DeepSeek-OCR Server Environment Variables
# Copy this file to .env and update the values

# Model Configuration
MODEL_PATH=/app/models/deepseek-ai/DeepSeek-OCR
GPU_MEMORY_UTILIZATION=0.85
CUDA_VISIBLE_DEVICES=0

# Server Configuration
HOST=0.0.0.0
PORT=8000
WORKERS=1

# Runpod Configuration (if using Runpod)
RUNPOD_POD_ID=your-pod-id
RUNPOD_API_KEY=your-api-key
RUNPOD_ENDPOINT_URL=https://your-pod-endpoint.runpod.net

# Optional: Hugging Face for model downloads
HF_TOKEN=your-huggingface-token

# Logging
LOG_LEVEL=INFO

# Performance
MAX_CONCURRENT_REQUESTS=10
REQUEST_TIMEOUT=300